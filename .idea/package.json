{
  "name": "search-cofa-sds-test",
  "version": "1.0.0",
  "description": "Search Cofa Sds Test",
  "main": "index.js",
  "scripts": {
    "cy:run": "yarn cypress run --config video=false,integrationFolder=cypress/integration --env allure=true --browser chrome",
    "cypress:run": "cypress run --env allure=true",
    "cypress:open": "cypress open",
    "clean:reports": "rm -rf cypress/results/* cypress/screenshots/* cypress/videos/*",
    "scripts": "cypress run --browser chrome ; npm run reports",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/results/mochareports/screenshots || :",
    "report:copyVideos": "cp -r cypress/videos cypress/results/mochareports/videos || :",
    "test": "npm run scripts ",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.qiagen.ads/scm/qat/search-cofa-sds-test.git"
  },
  "author": "Maciej Wawryczuk ",
  "license": "ISC",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.0.0",
    "@shelex/cypress-allure-plugin": "^2.19.1",
    "cypress": "^8.5.0",
    "cypress-file-upload": "^4.1.1",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-xpath": "^1.6.2",
    "mocha": "^8.0.1",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "chai": "^4.3.4",
    "cypress-downloadfile": "^1.2.1"
  }
}
